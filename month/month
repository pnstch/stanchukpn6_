package month;

public enum Months {

    JANUARY("Январь", 1),
    FEBRUARY("Февраль", 2),
    MARCH("Март", 3),
    APRIL("Апрель", 4),
    MAY("Май", 5),
    JUNE("Июнь", 6),
    JULY("Июль", 7),
    AUGUST("Август", 8),
    SEPTEMBER("Сентябрь", 9),
    OCTOBER("Октябрь", 10),
    NOVEMBER("Ноябрь", 11),
    DECEMBER("Декабрь", 12);

    Months(String name, int serialNumber) {
        this.name = name;
        this.serialNumber = serialNumber;
    }

    @Override
    public String toString() {
        return "Month{" +
                "name='" + name + '\'' +
                ", serialNumber=" + serialNumber +
                '}';
    }

    private String name;
    private int serialNumber;
}